   ],
   "source": [
    "# Here, i = first number\n",
    "#       j=Second number\n",
    "i=0\n",
    "j=1\n",
    "for n in range(0,9):\n",
    "    if n==1:\n",
    "        k=n\n",
    "    else:\n",
    "        k=i+j\n",
    "        i=j\n",
    "        j=k\n",
    "    print(k,end=' ')"
   ]
